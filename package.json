    "src"
  ],
  "homepage": "https://github.com/bokub/linky-cli",
  "keywords": [
    "api",
    "cli",
    "consumption",
    "enedis",
    "linky",
    "power"
  ],
  "license": "GPL-3.0-or-later",
  "main": "index.js",
  "repository": "https://github.com/bokub/linky-cli",
